<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                            http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID"
         version="3.0">

  <display-name>Projekt</display-name>

  <welcome-file-list>
    <welcome-file>Aufgabe2.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ServletStart</servlet-name>
    <servlet-class>Servlet.Setstart</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ServletStart</servlet-name>
    <url-pattern>/ServletStart</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletEnd</servlet-name>
    <servlet-class>Servlet.Setend</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ServletEnd</servlet-name>
    <url-pattern>/ServletEnd</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RoutenplanungServlet</servlet-name>
    <servlet-class>Servlet.RoutenplanungServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoutenplanungServlet</servlet-name>
    <url-pattern>/RoutenplanungServlet</url-pattern>
  </servlet-mapping>




</web-app>
